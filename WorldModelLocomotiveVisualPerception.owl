    <!-- http://www.semanticweb.org/jgr/ontologies/2022/1/KI-LOK-scene-description#VehicleCanHaveAd -->

    <owl:Class rdf:about="http://www.semanticweb.org/jgr/ontologies/2022/1/KI-LOK-scene-description#VehicleCanHaveAd">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/jgr/ontologies/2022/1/KI-LOK-scene-description#RoadVehicle"/>
                    <owl:Class>
                        <owl:complementOf rdf:resource="http://www.semanticweb.org/jgr/ontologies/2022/1/KI-LOK-scene-description#SpecialVehicle"/>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/jgr/ontologies/2022/1/KI-LOK-scene-description#hasVehicleBodyType"/>
                        <owl:allValuesFrom>
                            <owl:Class>
                                <owl:oneOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://www.semanticweb.org/jgr/ontologies/2022/1/KI-LOK-scene-description#VBTBus"/>
                                    <rdf:Description rdf:about="http://www.semanticweb.org/jgr/ontologies/2022/1/KI-LOK-scene-description#VBTCamperVan"/>
                                    <rdf:Description rdf:about="http://www.semanticweb.org/jgr/ontologies/2022/1/KI-LOK-scene-description#VBTCar"/>
                                    <rdf:Description rdf:about="http://www.semanticweb.org/jgr/ontologies/2022/1/KI-LOK-scene-description#VBTTruck"/>
                                    <rdf:Description rdf:about="http://www.semanticweb.org/jgr/ontologies/2022/1/KI-LOK-scene-description#VBTVan"/>
                                </owl:oneOf>
                            </owl:Class>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/jgr/ontologies/2022/1/KI-LOK-scene-description#RoadVehicle"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/jgr/ontologies/2022/1/KI-LOK-scene-description#hasAdvertisement"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:oneOf>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</rdf:first>
                                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </owl:oneOf>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/jgr/ontologies/2022/1/KI-LOK-scene-description#VehicleCannotHaveAd"/>
        <rdfs:comment>only vehicles with these body types can have ads. If anything else is assigned hasAdv. = true, ontology will be inconsistant</rdfs:comment>
    </owl:Class>
    


